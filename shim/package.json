{
  "name": "intellispace_plugin_shim",
  "scripts": {
    "clean": "del-cli ./prod/*",
    "copy": "cpx src/shim.html prod/ && cpx src/lib/** prod/lib",
    "compile_typescript": "tsc",
    "copy_to_svelte": "del-cli --force ../frontend/static/plugin2/** && cpx ./prod/** ../frontend/static/plugin2/",
    "prebuild": "npm-run-all clean copy",
    "build": "npm run compile_typescript",
    "postbuild": "npm-run-all copy_to_svelte"
  },
  "dependencies": {
    "cpx": "^1.5.0",
    "del-cli": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.5.3"
  }
}
