{
	"name": "application",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"tauri": "tauri",
		"tauri-dev": "WEBKIT_DISABLE_DMABUF_RENDERER=1 tauri dev",
		"tauri-dev-win": "tauri dev",
		"tauri-dev-nix": "nix-shell --command 'npm run tauri-dev' ../default.nix",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme/light",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@material/typography": "^14.0.0",
		"@mdi/js": "^7.4.47",
		"@smui-extra/autocomplete": "^7.0.0-beta.15",
		"@smui/button": "^7.0.0-beta.15",
		"@smui/checkbox": "^7.0.0-beta.16",
		"@smui/dialog": "^7.0.0-beta.15",
		"@smui/drawer": "^7.0.0-beta.15",
		"@smui/form-field": "^7.0.0-beta.15",
		"@smui/icon-button": "^7.0.0-beta.15",
		"@smui/list": "^7.0.0-beta.15",
		"@smui/menu": "^7.0.0-beta.15",
		"@smui/radio": "^7.0.0-beta.15",
		"@smui/select": "^7.0.0-beta.15",
		"@smui/switch": "^7.0.0-beta.15",
		"@smui/textfield": "^7.0.0-beta.16",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@tauri-apps/api": "^1.6.0",
		"@tauri-apps/cli": "^2.0.0-beta.22",
		"@types/eslint": "^8.56.7",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"smui-theme": "^7.0.0-beta.8",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3"
	},
	"type": "module"
}